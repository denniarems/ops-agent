# ZapGap Docker Compose Environment Variables
# Copy this file to .env and fill in your actual values

# =============================================================================
# RUNTIME ENVIRONMENT
# =============================================================================
NODE_ENV=development

# =============================================================================
# MASTRA AGENT CONFIGURATION
# =============================================================================
# Upstash Redis Configuration
UPSTASH_REDIS_REST_URL=https://your-redis-url.upstash.io
UPSTASH_REDIS_REST_TOKEN=your_redis_token

# Upstash Vector Configuration
UPSTASH_VECTOR_REST_URL=https://your-vector-url.upstash.io
UPSTASH_VECTOR_REST_TOKEN=your_vector_token

# OpenRouter API Configuration
OPENROUTER_API_KEY=your_openrouter_api_key

# =============================================================================
# AWS CONFIGURATION
# =============================================================================
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================
DATABASE_URL=your_database_connection_string

# =============================================================================
# AUTHENTICATION (CLERK)
# =============================================================================
CLERK_SECRET_KEY=your_clerk_secret_key
CLERK_AUTHORIZED_PARTIES=your_authorized_parties

# =============================================================================
# SUPABASE CONFIGURATION
# =============================================================================
SUPABASE_KEY=your_supabase_key
SUPABASE_URL=https://your-project.supabase.co

# =============================================================================
# DEVELOPMENT NOTES
# =============================================================================
# For development, you can use placeholder values for services you're not using
# The docker-compose.yml includes fallback values for Mastra-related variables
# Make sure to set real values for services you plan to use
